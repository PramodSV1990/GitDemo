<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="test.Listners"></listener>
	</listeners>
	<parameter name="URL" value="amazon.com"></parameter>
  <test name="HomeLoan">
	  <parameter name="username" value="hloan"></parameter>
	  <parameter name="password" value="hpwd"></parameter>
	  <classes>
		  <class name="test.First">
			  <methods>
				  <exclude name="netBanking"/>
			  </methods>
		  </class>
		  <class name="test.Second">
			<methods>
			  <include name="mob.*"/>
		  </methods>			  
		  </class>
		  
	  </classes>
  </test>
  <test name="carloan">
	 <parameter name="username" value="cloan"></parameter>
	  <parameter name="password" value="cpwd"></parameter>
	  <classes>
		  <class name="test.Third"></class>
	  </classes>
  </test>
  
  <!-- Test -->
</suite> <!-- Suite -->
